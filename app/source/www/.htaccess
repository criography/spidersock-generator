RewriteCond %{REQUEST_URI} !(\.png|\.jpg|\.gif|\.jpeg|\.pdf|\.zip|\.7z|\.js|\.css)
RewriteCond %{HTTP_USER_AGENT} "MSIE 5" [or]
RewriteCond %{HTTP_USER_AGENT} "MSIE 6" [or]
RewriteCond %{HTTP_USER_AGENT} "MSIE 7"
RewriteRule . d.ie [L]

